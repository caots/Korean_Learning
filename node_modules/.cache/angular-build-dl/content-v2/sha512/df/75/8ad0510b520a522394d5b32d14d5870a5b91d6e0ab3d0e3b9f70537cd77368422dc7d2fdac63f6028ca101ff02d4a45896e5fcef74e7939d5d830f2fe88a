@Component({\n  selector: 'app-hira-writing',\n  templateUrl: './hira-writing.page.html',\n  styleUrls: ['./hira-writing.page.scss'],\n})\nexport class HiraWritingPage implements OnInit {\n  \n  @ViewChild('canvas') canvas: any;\n  @Input() width = 512;\n  @Input() height = 418;\n  canvasElement: HTMLCanvasElement ;\n  lastX: number;\n  lastY: number;\n\n  currentColour: string = '#333';\n  brushSize: number = 6;\n\n  constructor(public platform: Platform) { }\n\n  ngOnInit() {    \n  }\n\n  ngAfterViewInit(){\n    this.initCanvasDraw();\n  }\n\n  initCanvasDraw(){\n    this.canvasElement = this.canvas.nativeElement;\n    this.canvasElement.width = this.width;\n    this.canvasElement.height = this.height;\n  }\n\n  handleStart(ev){\n    this.lastX = ev.touches[0].pageX;\n    this.lastY = ev.touches[0].pageY;\n  }\n\n  changeColour(color, isdelete){\n    if(isdelete === true){\n      this.brushSize = 15;\n    }else{\n      this.brushSize = 6;\n    }\n    this.currentColour = color;    \n  }\n\n  handleMove(ev){\n\n    let ctx = this.canvasElement.getContext('2d');\n    let currentX = ev.touches[0].pageX;\n    let currentY = ev.touches[0].pageY;\n    \n    ctx.beginPath();\n    ctx.lineJoin = \"round\";\n    ctx.moveTo(this.lastX, this.lastY);\n    ctx.lineTo(currentX, currentY);\n    ctx.closePath();\n    ctx.strokeStyle = this.currentColour;\n    ctx.lineWidth = this.brushSize;\n    ctx.stroke();       \n\n    this.lastX = currentX;\n    this.lastY = currentY;\n\n  }\n\n}\n","import { NgModule } from '@angular/core';\nimport { Routes, RouterModule } from '@angular/router';\n\nimport { LearnWritingPage } from './learn-writing.page';\nimport { ROUTING_PAGE } from 'src/app/constants/config';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: LearnWritingPage\n  },\n  {\n    path: ROUTING_PAGE.WRITING.HIRA_WRITING,\n    loadChildren: () => import('./hira-writing/hira-writing.module').then( m => m.HiraWritingPageModule)\n  },\n];\n\n@NgModule({\n  imports: [